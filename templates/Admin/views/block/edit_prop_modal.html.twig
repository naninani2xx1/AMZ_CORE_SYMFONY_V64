{% embed 'Admin/partials/modal.html.twig' %}
    {% block title %}Edit Block{% endblock %}
    {% block modalClass %}modal-dialog-centered p-10 mw-900px{% endblock %}
    {% block body %}
        {{ form_start(form) }}
        <div class="fv-row mb-7">
            <!--begin::Input-->
            {{ form_rest(form) }}
            <!--end::Input-->
        </div>
        <button type="button" class="btn btn-primary"
                data-action="click->Admin--block-prop-edit#onSubmit">Submit</button>
        {{ form_end(form) }}
    {% endblock %}
{% endembed %}